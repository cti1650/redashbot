{
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended"
}
